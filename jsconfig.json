{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~auth/*": [
                "src/auth/*"
            ],
            "~products/*": [
                "src/products/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~ui/*": [
                "src/ui/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~context/*": [
                "src/auth/context/*"
            ],
            "~pages/*": [
                "src/auth/pages/*"
            ],
            "~reducers/*": [
                "src/auth/reducers/*"
            ],
            "~types/*": [
                "src/auth/types/*"
            ]
        }
    }
}