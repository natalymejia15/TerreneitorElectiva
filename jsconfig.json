{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~auth/*": [
                "src/auth/*"
            ],
            "~firebase/*": [
                "src/firebase/*"
            ],
            "~hooks/*": [
                "src/hooks/*"
            ],
            "~image/*": [
                "src/image/*"
            ],
            "~products/*": [
                "src/products/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~ui/*": [
                "src/ui/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}